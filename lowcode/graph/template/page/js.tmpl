{{- /* gotype: github.com/woocoos/kocli/lowcode/schema.JSSlot*/ -}}
{{- define "js-slot" -}}
    {{- if len .Params }}
        ({{ joinSlice .Params ","}}) => {
        return (
        {{- $type := .Value.TypeString }}
        {{- if eq $type "CompositeValueSlice" }}
            <>
            {{- range .Value.Value}}
                {{- template "composite-value" . }}
            {{- end }}
            </>
        {{- else }}
            {{ template "composite-value" .Value.Value }}
        {{- end }}
        )
        }
    {{- else }}
        {{ template "composite-value" .Value }}
    {{- end }}
{{- end -}}

{{- /* gotype: github.com/woocoos/kocli/lowcode/schema.JSExpression*/ -}}
{{- define "js-expression" -}}
    {{.Value}}
{{- end -}}

{{- /* gotype: github.com/woocoos/kocli/lowcode/schema.JSFunction*/ -}}
{{- define "js-function" -}}
    {{.Value}}
{{- end -}}